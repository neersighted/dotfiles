print_info() {
    info title
    info underline

    info "OS" distro
    info "Kernel" kernel
    info "Uptime" uptime
    info "Packages" packages
    info "Shell" shell
    info "Resolution" resolution
    info "DE" de
    info "WM" wm
    info "WM Theme" wm_theme
    info "Theme" theme
    info "Icons" icons
    info "Font" font
    info "Terminal" term
    info "Terminal Font" term_font
    info "CPU" cpu
    info "GPU" gpu
    info "Memory" memory
    info "Song" song

    #info "Model" model
    #info "CPU Usage" cpu_usage
    #info "Disk" disk
    #info "Battery" battery
    #info "Local IP" local_ip
    #info "Public IP" public_ip
    #info "Users" users
    #info "Install Date" install_date
    #info "Locale" locale

    info line_break
    info cols
    info line_break
}


