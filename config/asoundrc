defaults.pcm.rate_converter "speexrate_medium"

pcm.dmixed {
    type dmix

    ipc_key_add_uid true
    ipc_key 5678293
    ipc_perm 0660
    ipc_gid audio

    slave {
        channels 2
        period_size 1024
        buffer_size 8192
        pcm {
            @func getenv
            vars [ ALSA_PCM ]
            default "hw"
        }
    }
}

pcm.!default {
    type plug
    slave.pcm "dmixed"
}
