defaults.pcm.rate_converter "speexrate_medium"

pcm.!default {
    type asym
    capture.pcm {
        @func getenv
        vars [ ALSA_IN ]
        default "pulse"
        fallback "sysdefault"
    }
    playback.pcm {
        @func getenv
        vars [ ALSA_OUT ]
        default "pulse"
        fallback "sysdefault"
    }
}
